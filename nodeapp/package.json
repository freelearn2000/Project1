{
  "name": "nodeapp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "test": "jest",
    "test:coverage": "jest --collectCoverage",
    "test:coverage1": "jest --silent --testLocationInResults --ci --all --coverage ",
    "start1": "ts-node src/index.ts",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node src/index.js",
    "lint": "eslint . --ext .ts & eslint **/*.ts -f html -o ./reports/lint-results.html "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-mung": "^0.5.1",
    "nconf": "^0.11.3",
    "njwt": "^1.1.0",
    "pg": "^8.7.1",
    "pg-mem": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "run": "^1.4.0",
    "typeorm": "^0.3.6",
    "winston": "^3.3.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@jest/types": "^27.5.1",
    "@types/express": "^4.17.13",
    "@types/express-mung": "^0.5.2",
    "@types/nconf": "^0.10.1",
    "@types/node": "^17.0.26",
    "@types/supertest": "^2.0.11",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.5.1",
    "jest-html-reporter": "^3.4.2",
    "semantic-release": "^19.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/?(*.)+(test).ts",
      "!**/config/**",
      "!**/index.ts",
      "!**/server.ts"
    ],
    "resetMocks": true,
    "clearMocks": true,
    "verbose": true,
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/index.ts",
      "!**/server.ts",
      "!**/typeorm.config.ts",
      "!**/src/models/**",
      "!**/src/middlewares/**",
      "!**/src/shared/**",
      "!**/config/**"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 50,
        "statements": 50,
        "branches": 25,
        "lines": 50
      }
    },
    "coverageReporters": [
      "html"
    ],
    "coverageDirectory": "./reports/coverage",
    "testResultsProcessor": "./node_modules/jest-html-reporter"
  }
}
